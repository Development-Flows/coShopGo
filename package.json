{
  "name": "coShopGo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "server/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
